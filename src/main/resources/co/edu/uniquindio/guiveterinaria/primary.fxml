<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.guiveterinaria.viewController.PrimaryController">
    <children>
        <Label text="Bienvenido a la Veterinaria" />
        <Button fx:id="primaryButton" onAction="#onOpenCrudCliente" text="Gestión de clientes" />
      <Button fx:id="primaryButton1" onAction="#onOpenCrudMascota" text="Gestión de mascotas" />
      <Button fx:id="primaryButton2" onAction="#onOpenCrudConsultaMedica" text="Gestión de consultas" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
