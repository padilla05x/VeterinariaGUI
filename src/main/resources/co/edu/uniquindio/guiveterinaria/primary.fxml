<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.guiveterinaria.viewController.PrimaryController">
    <children>
        <Label text="¡Bienvenido a la Veterinaria!" textAlignment="CENTER" />
        <Button fx:id="primaryButton" onAction="#onOpenCrudCliente" prefHeight="25.0" prefWidth="140.0" text="Gestión de clientes" />
      <Button fx:id="primaryButton1" onAction="#onOpenCrudMascota" prefHeight="25.0" prefWidth="138.0" text="Gestión de mascotas" />
      <Button fx:id="primaryButton3" onAction="#onOpenCrudVeterinario" prefHeight="25.0" prefWidth="138.0" text="Gestión de veterinarios" />
        <Button fx:id="primaryButton2" onAction="#onOpenCrudConsultaMedica" prefHeight="25.0" prefWidth="138.0" text="Gestión de consultas" />

    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
